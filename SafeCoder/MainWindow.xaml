                <EasingThicknessKeyFrame KeyTime="0:0:0.5" Value="112.5,5,72.5,0"/>
                <EasingThicknessKeyFrame KeyTime="0:0:0.7" Value="0,5,0,0"/>
                <EasingThicknessKeyFrame KeyTime="0:0:1" Value="0,5,0,0"/>
            </ThicknessAnimationUsingKeyFrames>
        </Storyboard>
